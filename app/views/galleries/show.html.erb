<% provide(:title, @gallery.name) %>
<div class="row">
  <h3><%= @gallery.name %> (<%= @gallery.pictographs.count %>) </h3></br>
  <% if @gallery.pictographs.any? %>
    <div class="polaroid">
      <%= render @pictographs %>
      <%= will_paginate @pictographs %>
    </div>
  <% end %>
</div>
